{
    "name": "digital-ocean-api-node",
    "version": "0.0.14",
    "description": "A package for interacting with the digital ocean api",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mikejg101/digital-ocean-api.git"
    },
    "scripts": {
        "clean": "./node_modules/.bin/rimraf ./dist/*",
        "test": "./node_modules/.bin/jest",
        "build": "npm run clean && ./node_modules/.bin/tsc",
        "format": "./node_modules/.bin/prettier '*/**/*.{js,ts,tsx}' --write",
        "lint": "./node_modules/.bin/eslint '*/**/*.{js,ts,tsx}' --fix",
        "prepare": "husky install && npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "keywords": [
        "digital",
        "ocean",
        "api"
    ],
    "author": "Michael Goodwin",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.27.2",
        "camelcase": "^6.3.0",
        "jest": "^28.1.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
        "collectCoverage": true
    },
    "devDependencies": {
        "@types/jest": "^27.5.1",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.1",
        "jest-mock-axios": "^4.6.1",
        "lint-staged": "^12.4.1",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    }
}
